    "def sum_eo(n, t):\n",
    "    if t == \"e\":\n",
    "        start = 2\n",
    "    elif t == 'o':\n",
    "        start = 1\n",
    "    else:\n",
    "        return -1\n",
    "\n",
    "    return sum(range(start, n, 2))\n",
    "\n",
    "\n",
    "x = sum_eo(11, 'e')\n",
    "print(x)"
   ]
